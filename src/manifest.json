{
    "manifest_version": 3,
    "name": "${description}",
    "description": "Simplifies adding torrent, ed2k, ftp files to ASUS Download Master queue",
    "version": "${version}",
    "permissions": [
        "storage",
        "contextMenus",
        "notifications"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {},
    "options_page": "opts.html",
    "icons": {
        "64": "icon.png"
    }
}