{
    "manifest_version": 2,
    "name": "ASUS Download Master Chrome Extension",
    "description": "Download files using ASUS Download Master",
    "version": "1.0.0",
    "permissions": [
        "storage",
        "contextMenus",
        "notifications",
        "webRequest",
        "webRequestBlocking",
        "http://*/",
        "https://*/"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "options_page": "options.html",
    "browser_action": {},
    "icons": {
        "66": "icon.png"
    }
}